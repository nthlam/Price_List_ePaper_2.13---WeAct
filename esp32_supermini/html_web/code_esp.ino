void handleRoot() {
  server.send(200, "text/html", processHTML(formHTML));
}

void handleSubmit() {
  if (server.hasArg("name")) name = server.arg("name");
  if (server.hasArg("price")) price = server.arg("price");
  server.send(200, "text/html", processHTML(formHTML));
  // GỬI DỮ LIỆU QUA SERIAL
  Serial.println(name); capnhat(  name, 40, 50, 150,20);
  Serial.println(price); capnhat(  price, 40, 90, 150,20);
}

void setup() {
  // khoi tao serial + led
  pinMode(8, OUTPUT);
  digitalWrite(8, HIGH);
  Serial.begin(115200);

  // tạo ĐIỂM KẾT NỐI WF
  WiFi.softAP(ssid, password);
  Serial.println("Access Point IP: " + WiFi.softAPIP().toString());
  String ip_add = WiFi.softAPIP().toString();
  server.on("/", handleRoot);          // gán endpoint với hàm 
  server.on("/submit", handleSubmit);
  server.begin();

  digitalWrite(8, LOW);
  // KHỞI TẠO MÀN HÌNH
  display.init(115200, true, 50, false);
  Serial.println("Da init Word...");
  //--//
  display.setRotation(1);
  display.setFullWindow();
  display.firstPage();
do {
  display.setFont(&FreeMonoBold9pt7b);
  display.setTextColor(GxEPD_BLACK);
  display.setCursor(100, 65);
  display.println("IP:");
  display.println(ip_add);
} while (display.nextPage());
 
  delay(5000);
  // IN TEMPLATE 
  display.firstPage();
  do {
    display.fillScreen(GxEPD_WHITE);
    // display.drawBitmap(10, 10, image_data_logo, 150, 50, GxEPD_BLACK);
    display.drawBitmap(0, 0, image_data_vinmart, 250, 122, GxEPD_RED);
    // display.drawBitmap(0, 0, VN_flag_bitmap , 250, 122, GxEPD_RED);
  } while (display.nextPage());
  /////
  digitalWrite(8, HIGH);

}

void loop() {
  server.handleClient();
}

void capnhat(String text, int x, int y, int ngang, int doc){
  display.setPartialWindow(x, y, ngang, doc);
  display.firstPage();
  do {
    display.setFont(&FreeMonoBold9pt7b);
    display.setTextColor(GxEPD_BLACK);
    display.setCursor(x, y +  doc/2 + 5 );
    display.print(text);
  } while (display.nextPage());
}
